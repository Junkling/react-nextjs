{
    "parser": "barbel-eslint",
    "paserOptions": {
        "ecmaVersion" : 2020,
        "sourceType" : "module",
        "ecmaFeatures": {
            "jsx" : true
        }
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "airbnb"
    ],
    "plugins": [
        "import",
        "react-hooks",
        "jsx-a11y"
    ],
    "rules": {
        "no-param-reassign": "off"
    },
    "ignorePatterns": ["dist/", "node_modules/"]
}